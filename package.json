{
  "author": "FancyStudio",
  "dependencies": {
    "@sapphire/result": "2.6.6",
    "es-toolkit": "1.17.0",
    "js-yaml": "4.1.0",
    "ts-pattern": "5.3.1",
    "zod": "3.23.8"
  },
  "description": "FancyScript is an easy-to-use syntax for creating custom actions for Discord bots using YAML",
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/js-yaml": "4.0.9",
    "@types/node": "22.5.2",
    "husky": "9.1.5",
    "js-yaml": "4.1.0",
    "lint-staged": "15.2.10",
    "tsx": "4.19.0",
    "typescript": "5.5.4",
    "vitest": "2.0.5"
  },
  "imports": {
    "#types": "./src/types.js",
    "#util/*": "./src/util/*"
  },
  "keywords": [],
  "license": "BSD-3-Clause",
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": ["pnpm biome"]
  },
  "name": "fancyscript",
  "packageManager": "pnpm@9.8.0+sha256.56a9e76b51796ca7f73b85e44cf83712862091f4d498c0ce4d5b7ecdc6ba18f7",
  "scripts": {
    "biome": "biome check --write --config-path=\"./biome.json\" .",
    "build": "tsc",
    "dev": "tsx --env-file=\".env\" --watch src/index.ts",
    "docs:build": "pnpm --filter=fancyscriptdocumentation build",
    "docs:dev": "pnpm --filter=fancyscriptdocumentation dev",
    "docs:preview": "pnpm --filter=fancyscriptdocumentation preview",
    "prepare": "husky",
    "test": "vitest"
  },
  "version": "0.0.1"
}
