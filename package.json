{
  "author": "FancyStudio",
  "dependencies": {
    "emoji-regex": "10.4.0",
    "js-yaml": "4.1.0",
    "ts-pattern": "5.3.1",
    "zod": "3.23.8"
  },
  "description": "YAMLCord is an easy-to-use syntax for creating custom actions for Discord bots using YAML",
  "devDependencies": {
    "@biomejs/biome": "1.9.2",
    "@types/js-yaml": "4.0.9",
    "@types/node": "22.6.0",
    "husky": "9.1.6",
    "js-yaml": "4.1.0",
    "lint-staged": "15.2.10",
    "tsx": "4.19.1",
    "typescript": "5.6.2",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.1.1"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/types.d.ts"
    }
  },
  "files": ["dist"],
  "homepage": "https://yamlcord.pages.dev/",
  "keywords": [],
  "license": "CC BY-SA 4.0",
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": ["pnpm biome"]
  },
  "main": "dist/index.js",
  "name": "yamlcord",
  "packageManager": "pnpm@9.11.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/FancyStudioTeam/YAMLCord"
  },
  "scripts": {
    "biome": "biome check --write --config-path=\"./biome.json\" .",
    "build": "tsc",
    "dev": "tsx --env-file=\".env\" --watch src/index.ts",
    "docs:build": "pnpm --filter=docs build",
    "docs:dev": "pnpm --filter=docs dev",
    "docs:preview": "pnpm --filter=docs preview",
    "prepare": "husky",
    "test": "vitest --config=\"./vitest.config.mts\""
  },
  "types": "dist/types.d.ts",
  "version": "0.0.1-alpha"
}
